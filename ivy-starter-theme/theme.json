{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": false,
        "color": {
            "background": true,
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [
                {
					"colors": [ "#034539", "#00a88f" ],
					"slug": "primary-and-secondary",
					"name": "Primary and secondary"
				}
            ],
            "gradients": [
                {
					"slug": "diagonal-primary-to-secondary",
					"gradient": "linear-gradient(to bottom right,#034539 0%,#00a88f 100%)",
					"name": "Diagonal primary to secondary"
				}
            ],
            "palette": [
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#034539"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#00a88f"
                },
                {
                    "name": "Tertiary",
                    "slug": "tertiary",
                    "color": "#ffc452"
                },
                {
                    "name": "Light Primary",
                    "slug": "light-primary",
                    "color": "#e6eceb"
                },
                {
                    "name": "Light Secondary",
                    "slug": "light-secondary",
                    "color": "#e6f6f4"
                },
                {
                    "name": "Light Tertiary",
                    "slug": "light-tertiary",
                    "color": "#fff3dc"
                },
                {
                    "name": "Body Text",
                    "slug": "body-text",
                    "color": "#464d55"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                }
            ]
        },
        "layout": {
			"contentSize": "768px",
			"wideSize": "1200px"
		},
        "spacing": {
            "blockGap": null,
            "margin": false,
            "padding": false,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
        "typography": {
            "customFontSize": false,
            "dropCap": false,
            "fontFamilies": [
                {
					"fontFamily": "system-ui,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Droid Sans\",\"Helvetica Neue\",\"Fira Sans\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
            ],
            "fontSizes": [
				{
                    "name": "Tiny",
                    "size": "0.75rem",
                    "slug": "tiny"
                },
                {
                    "name": "Small",
                    "size": "0.9375rem",
                    "slug": "small"
                },
                {
                    "name": "Default",
					"size": "1.125rem",
					"slug": "normal"
				},
				{
                    "name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
                    "size": "clamp(2.25rem, 3vw, 2.75rem)",
					"slug": "x-large"
				}
			],
            "fontStyle": false,
            "fontWeight": false,
            "letterSpacing": false,
            "lineHeight": false,
            "textDecoration": false,
            "textTransform": false
        },
        "blocks": {
            "core/button": {
                "border": {
                    "radius": false
                }
            },
            "core/heading": {
                "typography": {
                    "fontSizes": []
                }
            }
        }
    },
    "styles": {
		"blocks": {},
        "color": {},
        "elements": {},
        "spacing": {}
    }
}